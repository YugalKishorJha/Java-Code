import java.util.ArrayList;
import java.util.List;

public class Basics {

	public static void main(String[] args) {
		
		List<Integer> ll= List.of(1,2,3,4,5,6);
		List<Integer> resEven=new ArrayList<>();
		for (Integer i : ll) {
			if(i%2==0)
				resEven.add(i);
		}
		System.out.println(ll);
		System.out.println(resEven);
	}

}
