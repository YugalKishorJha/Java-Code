package com.JSP.HQLQuery;

public class Employee {
	private int id;
	private String name;
	private double sal;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getPerc() {
		return sal;
	}

	public void setPerc(double perc) {
		this.sal = perc;
	}

}
